    <!--Style statistics content-->
    <Style x:Key="StyleButtonStatsTime" TargetType="{x:Type Button}">
        <Setter Property="Background" Value="#D9D9D9"/>
        <Setter Property="Foreground" Value="#5F5F5F"/>

        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Border Background="{TemplateBinding Background}" CornerRadius="5">
                        <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" TextElement.FontFamily="pack://application:,,,/Fonts/#Baloo Paaji 2"/>
                    </Border>

                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter Property="Background" Value="#BB0144"/>
                            <Setter Property="Foreground" Value="#D6D6D6"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

